endif
CFLAGS	       += $(shell pkg-config gtk+-2.0 --cflags)
CCFLAGS		= $(CFLAGS)

LIBS		= -lTUIeee1394++ -lTUTools++ \
		  -lraw1394 $(shell pkg-config gtk+-2.0 --libs)
ifneq ($(findstring darwin,$(OSTYPE)),)
  LIBS	       += -framework IOKit -framework CoreFoundation \
		  -framework CoreServices
endif

LINKER		= $(CXX)

